		suggestByIqiyi($("#headq").val(), loadSuggest);
	}else{
		if($("a#inputcancel").is(':visible')){
			$("a#inputcancel").trigger('click')
		}else{
			XBox.hide();
			$("a#inputcancel").trigger('click')
			$("#id_xbox div").first().show();
		}
	}
});
//实时监听输入框变化
