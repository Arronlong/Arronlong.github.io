<script type="text/javascript">
$("div.v").live("touchstart",function(){
	$("div.v").removeClass("current");
	$(this).addClass("current");
});
$("div.v").live("touchend",function(){
	$(this).removeClass("current");
});

$("div._sk_more").live("click",function(){
	var workscoll = $("div._sk_content");
	var lastvideo = $("div._sk_content div.v:last");
	var od = $("div._sk_content div.sub_tab li.on").attr("data_od") || "1";
	var site = $("div._sk_content a[data_site].on").attr("data_site") || "0";
	var lt = $("div._sk_content a[data_lt].on").attr("data_lt") || "0";
	var hd = $("div._sk_content a[data_hd].on").attr("data_hd") || "0";
	var kw = $.trim(lastvideo.attr("keyword"));
	var nextpage = lastvideo.attr("nextpage");
	var needrequest = lastvideo.attr("needrequest");
	if(kw == "" || needrequest != "t") return;

	var loading = $('<div class="v-loading _sk_load">正在加载 ...<em class="v-loading32"></em></div>');
	var more = $("<div class=\"v-load _sk_more\">展开更多</div>")
	$.skAjax({
		url:"https://www.soku.com/m/y/video_ajax",
		data:{"q":kw,"site":site,"cp":nextpage,"ps":20,"od":od,"lt":lt,"hd":hd,"noqc":"0"},
		beforeSend:function(){
			workscoll.find("div._sk_more").remove();
			loading.appendTo(workscoll);
		},
		success:function(tab){
			workscoll.find("div._sk_load").remove();
			if($.trim(tab) == ""){
				if(lastvideo[0]){
					lastvideo.attr("needrequest","f");
					$("div._sk_more").remove();
				}
			}else{
				$(tab).appendTo(workscoll);
				var last = workscoll.find("div.v:last");
				if(last[0] && "t" == last.attr("needRequest")){
					more.appendTo(workscoll);
				}
			}
		},
		error:function(){
			workscoll.find("div._sk_load").remove();
		}
	});
});
</script>

